{
  "extends": "@seoko/tsconfig/react.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": { "@/*": ["./src/*"] },
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"]
}
